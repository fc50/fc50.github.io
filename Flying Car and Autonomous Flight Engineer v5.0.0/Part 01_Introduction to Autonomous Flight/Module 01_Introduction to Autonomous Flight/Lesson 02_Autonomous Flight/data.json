              "question": {
                "prompt": "Why do the two pairs of propellers on a quadrotor spin in opposite directions?",
                "answers": [
                  {
                    "id": "a1511745021138",
                    "text": "One pair is for driving air up and the other is for driving air down.",
                    "is_correct": false
                  },
                  {
                    "id": "a1511745092361",
                    "text": "It makes no difference, they could just as well be all spinning in the same direction",
                    "is_correct": false
                  },
                  {
                    "id": "a1511745113946",
                    "text": "One pair is for moving the vehicle forward and the other for moving it backward.",
                    "is_correct": false
                  },
                  {
                    "id": "a1511745223070",
                    "text": "The net torque from the two pairs of propellers cancels out so the vehicle is stable in flight.",
                    "is_correct": true
                  }
                ]
              }
            }
          ]
