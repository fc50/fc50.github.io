            {
              "id": 676191,
              "key": "60b1b6c8-55d7-40d4-b830-10f4816eb195",
              "title": "Nd787 C5 L1 A03 L Fixed Wing Vs Rotary Wing Aircraf V2",
              "semantic_type": "VideoAtom",
              "is_public": true,
              "instructor_notes": "",
              "video": {
                "youtube_id": "3YCIT9xUgrQ",
