            {
              "id": 626819,
              "key": "1b5a2688-b4d3-43f0-9892-b4833f5b31ef",
              "title": "Nd787 C3 L4 A02 Review Of 2D Dynamics V1",
              "semantic_type": "VideoAtom",
              "is_public": true,
              "instructor_notes": "",
              "video": {
                "youtube_id": "AhdEPjfhFWc",
