            {
              "id": 571353,
              "key": "19959d66-2449-4337-a847-c93f4459e8b3",
              "title": "Nd787 C3 L4 A06 Eulers Equations In A Rotating Frame V1",
              "semantic_type": "VideoAtom",
              "is_public": true,
              "instructor_notes": "",
              "video": {
                "youtube_id": "7VyzQGlax_0",
